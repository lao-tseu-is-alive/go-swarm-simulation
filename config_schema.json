{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "worldWidth": {
      "type": "number",
      "minimum": 100
    },
    "worldHeight": {
      "type": "number",
      "minimum": 100
    },
    "numRedAtStart": {
      "type": "integer",
      "minimum": 0
    },
    "numBlueAtStart": {
      "type": "integer",
      "minimum": 0
    },
    "detectionRadius": {
      "type": "number",
      "minimum": 0
    },
    "defenseRadius": {
      "type": "number",
      "minimum": 0
    },
    "contactRadius": {
      "type": "number",
      "minimum": 0
    },
    "maxSpeed": {
      "type": "number",
      "minimum": 1.0,
      "maximum": 10
    },
    "aggression": {
      "type": "number",
      "minimum": 0,
      "maximum": 5
    },
    "visualRange": {
      "type": "number",
      "minimum": 0
    },
    "protectedRange": {
      "type": "number",
      "minimum": 0
    },
    "centeringFactor": {
      "type": "number",
      "minimum": 0
    },
    "avoidFactor": {
      "type": "number",
      "minimum": 0
    },
    "matchingFactor": {
      "type": "number",
      "minimum": 0
    },
    "turnFactor": {
      "type": "number",
      "minimum": 0
    },
    "minSpeed": {
      "type": "number",
      "minimum": 0
    }
  },
  "required": [
    "worldWidth",
    "worldHeight",
    "numRedAtStart",
    "numBlueAtStart",
    "detectionRadius",
    "defenseRadius",
    "contactRadius",
    "maxSpeed",
    "aggression",
    "visualRange",
    "protectedRange",
    "centeringFactor",
    "avoidFactor",
    "matchingFactor",
    "turnFactor",
    "minSpeed"
  ]
}